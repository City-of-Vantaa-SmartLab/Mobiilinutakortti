{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "compodoc": "npx compodoc -s",
    "compodoc:build": "npx compodoc -p ./tsconfig.json -n 'API'",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "rm ./test/testdb.sql || true && cross-env NODE_ENV=test jest --runInBand && rm ./test/testdb.sql",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand && rm ./test/testdb.sql"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.352.0",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.6",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/platform-socket.io": "^8.4.6",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.1.3",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/prettier": "^2.6.3",
    "@types/qs": "^6.9.7",
    "@types/saml2-js": "^1.6.8",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "moment": "^2.24.0",
    "nestjs-pino": "^3.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pixl-xml": "^1.0.13",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "saml2-js": "^4.0.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@babel/parser": "^7.9.4",
    "@babel/types": "^7.9.5",
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/bcrypt": "^3.0.1",
    "@types/cron": "^2.0.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^16.18.36",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "sqlite3": "^5.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.1.1",
    "tslint": "^5.20.1",
    "typescript": "^4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
