{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "compodoc": "npx compodoc -s",
    "compodoc:build": "npx compodoc -p ./tsconfig.json -n 'API'",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "rm ./test/testdb.sql || true && set NODE_ENV=test&& jest --runInBand && rm ./test/testdb.sql",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "set NODE_ENV=test&& jest --config ./test/jest-e2e.json --runInBand && rm ./test/testdb.sql"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-socket.io": "^7.6.15",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.15",
    "@types/saml2-js": "^1.6.8",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.10.2",
    "cookie-parser": "^1.4.5",
    "moment": "^2.24.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "pixl-xml": "^1.0.13",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "saml2-js": "^3.0.1",
    "socket.io": "^2.4.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@babel/parser": "^7.9.4",
    "@babel/types": "^7.9.5",
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
