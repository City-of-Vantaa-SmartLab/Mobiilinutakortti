{
  "name": "backend",
  "version": "2.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "rm ./test/testdb.sql 2>/dev/null || true && cross-env NODE_ENV=test jest --runInBand && rm ./test/testdb.sql 2>/dev/null",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "rm ./test/testdb.sql 2>/dev/null || true && cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand && rm ./test/testdb.sql 2>/dev/null"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.352.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.0.3",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.6.0",
    "moment": "^2.24.0",
    "nestjs-pino": "^3.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pixl-xml": "^1.0.13",
    "rxjs": "^7.5.5",
    "saml2-js": "^4.0.2",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cookie-signature": "^1.1.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.1.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/pixl-xml": "^1.0.5",
    "@types/saml2-js": "^3.0.4",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "sqlite3": "^5.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
